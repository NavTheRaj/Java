public class  ContinueBreak2{
  public static void main(String[]args)
  {
    Loop1:for(int i=1;i<100;i++)
    {
      System.out.println("");
      if(i>=10)
      {
        break;
      }
      
      for(int j=1;j<10;j++)
      {
        System.out.print("*");
        if(j==1)
        {
          continue Loop1;
        }
      }
    }
  
  
  System.out.println("Terminated by Break");
}
}

     
