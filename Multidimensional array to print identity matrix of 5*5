//import java.util.Scanner;

class MultiDimensionalArray{
  public static void main(String args[]){
  int table[][]=new int[5][5];
    for(int i=0;i<5;i++)
    {
      for(int j=0;j<5;j++)
      {
        if(i==j)
        {
          System.out.print("   " +(table[i][j]=1));
         }
else
{
  System.out.print("   " +(table[i][j]=0));
                     }
}
System.out.println();

}
  }
}

  
